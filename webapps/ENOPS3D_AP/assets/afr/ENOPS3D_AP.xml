<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Template Target="CATCmdHeadersList" syp:name="ENX3DSCWkbHeaders">
    <CATCmdHeadersList>
      <CATCommandHeader ID="AddRoot" ClassName="DS/PADUtils/commands/PADAddRoot_cmd" ResourceFile="PADUtils/PADUtils" Available="1"/>
      <CATCommandHeader ClassName="DS/ENOWebNavVolumeQuery/ENOWebNavVolumeQueryCmd" ID="VolumeQueryHdr" ResourceFile="ENOWebNavVolumeQuery/ENOWebNavVolumeQueryCmd"/>
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerSwitchVisuOnDemandCmd" ID="SwitchVisuOnDemandHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerRemoveAllCmd" ID="RemoveAllHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader Behavior="CATAfrHeaderCheck" ClassName="DS/ENOPAD3DViewer/commands/PAD3DLineLayoutCmd" ID="PAD3DLineLayoutHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerRelationalNavigatorCmd" ID="PAD3DViewerRelationalNavigatorHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerNavigateOnSelectionCmd" ID="PAD3DViewerNavigateOnSelectionHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader ID="ActionBar_Attributes" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerEditPropertiesCmd" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer" Available="1"/>
      <CATCommandHeader Behavior="CATAfrHeaderCheck" ID="RootPanel" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerRootInfoPanelCmd" ResourceFile="ENXNav/ENXNav" Available="1"/>
      <CATCommandHeader Behavior="CATAfrHeaderCheck" ID="RightSidePanel" ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerRightSidePanel" ResourceFile="PADUtils/PADUtils" Available="1">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="PropertiesReadOnly" Value="true" Mode="In"/>
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <!--  Filtering commands -->
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerFilterDropObjectsCmd" ID="FilterDropObjects" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>
      <CATCommandHeader ClassName="DS/ENOPAD3DViewer/commands/PAD3DViewerDefineFilterCmd" ID="DefineFilterHdr" ResourceFile="ENOPAD3DViewer/ENOPAD3DViewer"/>

      <CATCommandHeader ClassName="DS/ViewerCommands/VisuVisibleSpaceCmd" ID="VisuVisibleSpace" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuReframeViewCmd" ID="VisuReframeView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuPanViewCmd" ID="VisuPanView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuZoomViewCmd" ID="VisuZoomView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuRotateViewCmd" ID="VisuRotateView" ResourceFile="ViewerCommands/ViewerCommands"/>

      <CATCommandHeader ClassName="DS/ViewerCommands/VisuIsoViewCmd" ID="VisuIsoView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuFrontViewCmd" ID="VisuFrontView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuBackViewCmd" ID="VisuBackView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuLeftViewCmd" ID="VisuLeftView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuRightViewCmd" ID="VisuRightView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuTopViewCmd" ID="VisuTopView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuBottomViewCmd" ID="VisuBottomView" ResourceFile="ViewerCommands/ViewerCommands"/>

      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingEdgesCmd" ID="VisuShadingEdges" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingCmd" ID="VisuShading" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingEdgesNoSmoothEdgesCmd" ID="VisuShadingEdgesNoSmoothEdges" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingEdgesHiddenEdgesCmd" ID="VisuShadingEdgesHiddenEdges" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingMaterialEdgesCmd" ID="VisuShadingMaterialEdges" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuShadingMaterialCmd" ID="VisuShadingMaterial" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuNoShadingEdgesCmd" ID="VisuNoShadingEdges" ResourceFile="ViewerCommands/ViewerCommands"/>

      <CATCommandHeader ClassName="DS/ViewerCommands/VisuPerspectiveViewCmd" ID="VisuPerspectiveView" ResourceFile="ViewerCommands/ViewerCommands"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuOrthographicViewCmd" ID="VisuOrthographicView" ResourceFile="ViewerCommands/ViewerCommands"/>

      <CATCommandHeader ClassName="DS/ViewerCommands/VisuFilterLinesCmd" ID="VisuFilterLines" ResourceFile="ViewerCommands/ViewerCommands" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuFilterAxisCmd" ID="VisuFilterAxis" ResourceFile="ViewerCommands/ViewerCommands" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ClassName="DS/ViewerCommands/VisuFilterPointsCmd" ID="VisuFilterPoints" ResourceFile="ViewerCommands/ViewerCommands" Behavior="CATAfrHeaderCheck"/>

      <CATCommandHeader ID="measureCmdHdr" ClassName="DS/DMUMeasure/DMUCreateMeasureCmd" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ProgressiveVisuLoading" Value="ProgressiveVisuLoading" Mode="In"/>
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="sectionCmdHdr" ClassName="DS/DMUSection/DMUCreateSectionCmd" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1">
        <CATAfrCommandHeaderArgs>
          <CATCommandHeaderArg ID="ProgressiveVisuLoading" Value="ProgressiveVisuLoading" Mode="In"/>
        </CATAfrCommandHeaderArgs>
      </CATCommandHeader>
      <CATCommandHeader ID="LockUnlockSectionHdr" ClassName="DS/DMUSection/DMULockUnlockSection" ResourceFile="DMUBaseCommands/3DPlayPro" Behavior="CATAfrHeaderCheck"/>
      <CATCommandHeader ID="contextualbarSectionHdr" ClassName="DS/DMUSection/DMUSectionContextCmd" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"/>
      <CATCommandHeader ID="contextualbarMeasureHdr" ClassName="DS/DMUMeasure/DMUMeasureContextCmd" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"/>

    </CATCmdHeadersList>
  </Template>

  <Template Target="CATCmdWorkbench" syp:name="ENX3DSCWkbAccess">
    <CATCmdWorkbench Name="ENX3DScene">

      <!-- View toolbar -->
      <CATCmdContainer Name="PADView" RepresentationType="AfrActionBarSection">
        <CATCmdStarter Name="RootPanel" Command="RootPanel"/>
        <CATCmdSeparator Name="PadViews_Separator"/>
        <CATCmdStarter Command="VisuReframeView" Name="PADPADReframeCmd"/>
        <CATCmdContainer Name="PADViewpointProjectionMode" RepresentationType="AfrActionBarDropDown" Visibility="1">
          <CATCmdStarter Command="VisuPerspectiveView" Name="PADVisuPerspectiveViewCmd"/>
          <CATCmdStarter Command="VisuOrthographicView" Name="PADVisuOrthographicView"/>
        </CATCmdContainer>
        <CATCmdStarter Command="VisuPanView" Name="PADPanCmd"/>
        <CATCmdStarter Command="VisuRotateView" Name="PADRotateCmd"/>
        <CATCmdStarter Command="VisuZoomView" Name="PADZoomCmd"/>

        <!-- Views -->
        <CATCmdContainer Name="PADCameraViews" RepresentationType="AfrActionBarDropDown" Visibility="1">
          <CATCmdStarter Command="VisuIsoView" Name="PADIsoViewCmd"/>
          <CATCmdStarter Command="VisuBottomView" Name="PADBottomViewCmd"/>
          <CATCmdStarter Command="VisuTopView" Name="PADTopViewCmd"/>
          <CATCmdStarter Command="VisuRightView" Name="PADRightViewCmd"/>
          <CATCmdStarter Command="VisuLeftView" Name="PADLeftViewCmd"/>
          <CATCmdStarter Command="VisuBackView" Name="PADBackViewCmd"/>
          <CATCmdStarter Command="VisuFrontView" Name="PADFrontViewCmd"/>
        </CATCmdContainer>

        <!-- Render Options -->
        <CATCmdContainer Name="PADRendering" RepresentationType="AfrActionBarDropDown" Visibility="1">
          <CATCmdStarter Command="VisuShading" Name="PADShading"/>
          <CATCmdStarter Command="VisuShadingEdges" Name="PADShadingWithEdges"/>
          <CATCmdStarter Command="VisuShadingEdgesNoSmoothEdges" Name="PADShadingWithEdgeAndNoSmoothEdges"/>
          <CATCmdStarter Command="VisuShadingEdgesHiddenEdges" Name="PADShadingWithEdgeAndHiddenEdges"/>
          <CATCmdStarter Command="VisuShadingMaterialEdges" Name="PADShadingMaterialWithEdges"/>
          <CATCmdStarter Command="VisuShadingMaterial" Name="PADShadingMaterial"/>
          <CATCmdStarter Command="VisuNoShadingEdges" Name="PADWireframe"/>
        </CATCmdContainer>
        <CATCmdContainer Name="PADSwitchLineIcElements" RepresentationType="AfrActionBarDropDown" Visibility="1">
          <CATCmdStarter Command="VisuFilterAxis" Name="VisuFilterAxis" Responsive="Medium Long Large"/>
          <CATCmdStarter Command="VisuFilterLines" Name="VisuFilterLines" Responsive="Medium Long Large"/>
          <CATCmdStarter Command="VisuFilterPoints" Name="VisuFilterPoints" Responsive="Medium Long Large"/>
        </CATCmdContainer>
        <CATCmdStarter Command="VisuVisibleSpace" Name="VisuVisibleSpaceCmd"/>
        <CATCmdSeparator Name="PadViews_Separator"/>
        <CATCmdStarter Command="RemoveAllHdr" Name="PADRemoveAll"/>

        <!-- Must be last -->
        <CATCmdSeparator Name="PadViews_Separator"/>
        <CATCmdStarter Command="RightSidePanel" Name="Right Side Panel"/>

      </CATCmdContainer>

      <!-- Tools toolbar -->
      <CATCmdContainer Name="PADTools" RepresentationType="AfrActionBarSection">
        <!--<CATCmdStarter Name="CAT3DComposeExamineSelectionStr" Command="CAT3DComposeExamineSelectionHdr"/> -->
        <CATCmdStarter Command="VolumeQueryHdr" Name="3DSelection"/>
        <CATCmdStarter Command="ActionBar_Attributes" Name="ActionBar_AttributesCmd"/>
        <!--<CATCmdStarter Command="PAD3DLineLayoutHdr" Name="PAD3DLineLayoutCmd"/>-->
        <CATCmdStarter Command="PAD3DViewerRelationalNavigatorHdr" Name="PAD3DViewerRelationalNavigatorCmd"/>
        <CATCmdStarter Command="DefineFilterHdr" Name="DefineFilterCmd"/>
        <CATCmdStarter Name="CAT3DComposeMeasureStr" Command="measureCmdHdr"/>
        <CATCmdStarter Name="CAT3DComposeSectionStr" Command="sectionCmdHdr"/>
        <CATCmdStarter Name="CAT3DComposeLockUnlockSectionStr" Command="LockUnlockSectionHdr"/>
      </CATCmdContainer>

    </CATCmdWorkbench>
  </Template>
</Styles>
