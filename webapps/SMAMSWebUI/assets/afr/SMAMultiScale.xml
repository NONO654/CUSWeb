	<Template syp:name="UNIWorkshopSIMAccess" Target="CATCmdWorkbench">
		<CATCmdWorkbench Name="MSworkbench"
			RepresentationType="AfrActionBar">
			<CATWorkbenchOptions ToolbarsExclusive="1" />
			<!-- CONFIGURATION SECTION -->
			<CATCmdContainer Name="Configuration"
				RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="MSExperiment" Command="SMAMultiScaleExpCmdHdr" />
				<CATCmdStarter Name="MSOpenExp" Command="SMAMultiScaleSearchExpCmdHdr" />
				<CATCmdStarter Name="MSClose" Command="SMAMultiScaleCloseCmdHdr" />
				<CATCmdStarter Name="MSEditExp" Command="SMAMultiScaleEditExpCmdHdr" />				
				<CATCmdSeparator Name="Sep1" />
				<CATCmdStarter Name="MSEmpty" Command="SMAMultiScaleEmptyCmdHdr" />
				<CATCmdStarter Name="MSAddComp" Command="SMAMultiScaleSearchCmdHdr" />
				<CATCmdStarter Name="MSEditComp" Command="SMAMultiScaleEditCompCmdHdr" />
				<CATCmdStarter Name="MSParam" Command="SMAMultiScaleParamCmdHdr" />
				<CATCmdStarter Name="MSDel" Command="SMAMultiScaleDelCmdHdr" />
			</CATCmdContainer>

			<!-- EXECUTION SECTION -->
			<CATCmdContainer Name="Execution" RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="MSExpParam" Command="SMAMultiScaleExpParamsCmdHdr" />
				<CATCmdStarter Name="MSBehSel" Command="SMAMultiScaleBehSelCmdHdr" />
				<CATCmdStarter Name="MSCloudExecOpt"   Command="SMAMultiScaleCloudExecOptCmdHdr" />
				<CATCmdStarter Name="MSCheck" Command="SMAMultiScaleCheckCmdHdr" />
				<CATCmdStarter Name="MSPlay" Command="SMAMultiScalePlayCmdHdr" />
				<CATCmdStarter Name="MSPlayRunAs" Command="SMAMultiScalePlayRunAsCmdHdr" />
				<CATCmdStarter Name="MSAbort" Command="SMAMultiScaleJobAbortCmdHdr" />
				<CATCmdStarter Name="MSStatus" Command="SMAMultiScaleStatusCmdHdr" />
				<CATCmdStarter Name="MSExpJobLog" Command="SMAMultiScaleJobLogCmdHdr" />
				<CATCmdStarter Name="MSExpExecOpt" Command="SMAMultiScaleExecOptCmdHdr" />
			</CATCmdContainer>

			<!-- RESULTS SECTION -->
			<CATCmdContainer Name="Results" RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="MSRealTime" Command="SMAMultiScaleRealTimeResultsCmdHdr" />
				<CATCmdStarter Name="MSGetResults" Command="SMAMultiScaleFileMonitoringCmdHdr" />
				<CATCmdStarter Name="MSGetMessages" Command="SMAMultiScaleGetMessagesCmdHdr" />
			</CATCmdContainer>

			<!-- VIEW SECTION -->
			<CATCmdContainer Name="View" RepresentationType="AfrActionBarSection">
				<CATCmdStarter Name="MSZoomIn" Command="SMAMultiScaleZoomInCmdHdr" />
				<CATCmdStarter Name="MSZoomOut" Command="SMAMultiScaleZoomOutCmdHdr" />
				<CATCmdStarter Name="MSRefram" Command="SMAMultiScaleReframCmdHdr" />
			</CATCmdContainer>

		</CATCmdWorkbench>
	</Template>
</Styles>


