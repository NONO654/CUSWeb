<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

      <Template syp:name="ENOFoldersHeaders" Target="CATCmdHeadersList">
            <CATCmdHeadersList>
                  <!-- OpenInCAD for web in win integration -->                 
      			  <CATCommandHeader ID="OpenInCADHdr" ClassName="DS/PADUtils/commands/PADOpenInCAD" ResourceFile="PADUtils/PADUtils" Available="1"/>
      
                  <!-- Folder Commands -->
                  <CATCommandHeader ID="ActionBar_NewFolderCmd" ClassName="DS/ENOCollabSharingCmds/commands/NewCmd/ActionBar_NewFolderCmd" ResourceFile="ENOCollabSharingCmds/CollabSharing_Workbench" Available="1" />
                  <CATCommandHeader ID="RemoveContent" ClassName="DS/FolderEditor/commands/ActionBar_RemoveContentCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="AddFile" ClassName="DS/FolderEditor/commands/ActionBar_AddFileCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="DownloadFile" ClassName="DS/FolderEditor/commands/ActionBar_DownloadFileCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="CheckOutFile" ClassName="DS/FolderEditor/commands/ActionBar_CheckOutCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="CheckInFile" ClassName="DS/FolderEditor/commands/ActionBar_CheckInCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="Cut" ClassName="DS/FolderEditor/commands/ActionBar_CutCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="Copy" ClassName="DS/FolderEditor/commands/ActionBar_CopyCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="Paste" ClassName="DS/FolderEditor/commands/ActionBar_PasteCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="AddExistingContent" ClassName="DS/FolderEditor/commands/ActionBar_AddExistingContentCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <CATCommandHeader ID="AddToFavorites" ClassName="DS/FolderEditor/commands/ActionBar_AddToFavoritesCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" Behavior="CATAfrHeaderCheck" />
                  <CATCommandHeader ID="ActiveFolder" ClassName="DS/FolderEditorCustomization/ActiveFolder/ActionBar_ActiveFolderCmd" ResourceFile="FolderEditor/ENOFolder" Available="1" />

                  <!-- PAD Commands -->
                  <CATCommandHeader ID="treelistview" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="graphview" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="tileview" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="thumbnailview" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>

                  <CATCommandHeader ID="ExpandAll" ClassName="DS/FolderEditor/commands/ActionBar_ExpandAllCmd" ResourceFile="FolderEditor/ENOFolder" Available="1"/>
                  <CATCommandHeader ID="ExpandN" ClassName="DS/FolderEditor/commands/ActionBar_ExpandNCmd" ResourceFile="FolderEditor/ENOFolder" Available="1"/>
                  <CATCommandHeader ID="CollapseAll" ClassName="DS/FolderEditor/commands/ActionBar_CollapseAllCmd" ResourceFile="FolderEditor/ENOFolder" Available="1"/>
                  <CATCommandHeader ID="ReframeOn" ClassName="DS/FolderEditor/commands/ActionBar_ReframeOnCmd" ResourceFile="FolderEditor/ENOFolder" Available="1"/>
                  <CATCommandHeader ID="FitAllIn" ClassName="DS/FolderEditor/commands/ActionBar_FitAllInCmd" ResourceFile="FolderEditor/ENOFolder" Available="1"/>

                  <CATCommandHeader ID="SelectChildren" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="SelectAll" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="SelectInversion" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="SelectParent" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="SelectSiblings" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>
                  <CATCommandHeader ID="SelectLeaves" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1"/>

				  <CATCommandHeader ID="ToggleLeftPanel" ClassName="DS/FolderEditor/commands/ActionBar_LeftTreePanelCmd" ResourceFile="FolderEditor/ENOFolder" Behavior="CATAfrHeaderCheck" Available="0"/>
                  <CATCommandHeader ID="ToggleOverview" ClassName="DS/FolderEditor/commands/ActionBar_ToggleOverview" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
                  <CATCommandHeader ID="LayoutDown" ClassName="DS/FolderEditor/commands/ActionBar_ToggleLayout" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
                  <CATCommandHeader ID="LayoutRight" ClassName="DS/FolderEditor/commands/ActionBar_ToggleLayout" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
                  <CATCommandHeader ID="LayoutUp" ClassName="DS/FolderEditor/commands/ActionBar_ToggleLayout" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
                  <CATCommandHeader ID="LayoutLeft" ClassName="DS/FolderEditor/commands/ActionBar_ToggleLayout" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
                  <CATCommandHeader ID="SwapThumbState" ClassName="DS/FolderEditor/commands/ActionBar_SwapThumbState" ResourceFile="FolderEditor/ENOFolder" Available="0"/>
				  <CATCommandHeader ID="ToggleRightPanel" ClassName="DS/FolderEditor/commands/ActionBar_PropertySidePanelCmd" ResourceFile="FolderEditor/ENOFolder" Behavior="CATAfrHeaderCheck" Available="0"/>
				  
                  <!-- Tools Commands -->
                  <CATCommandHeader ID="ColumnVisibility" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav"/>
                  <CATCommandHeader ID="Print" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="PrintOptions" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0"/>
                  <CATCommandHeader ID="WhereUsed" ClassName="DS/FolderEditor/commands/ActionBar_RelationIPExplorer" ResourceFile="FolderEditor/ENOFolder" Available="1" />
                  <!-- Other Commands -->
                  <CATCommandHeader ID="ActionBar_Attributes" ClassName="DS/ENOCollabSharingCmds/commands/EditPropCmd/ActionBar_EditPropCmd" ResourceFile="ENOCollabSharingCmds/CollabSharing_Workbench" Available="1" />
                  <CATCommandHeader ID="DeleteHdr" ClassName="DS/LifecycleCmd/DeleteCmd" ResourceFile="LifecycleCmd/DeleteCmd" Available="1"/>
                  <!-- LOGICAL DELETE COMMANDS -->
                  <CATCommandHeader ID="DeleteDeletedItemsHdr" ClassName="DS/LifecycleCmd/DeleteDeletedItemsCmd" ResourceFile="LifecycleCmd/DeleteDeletedItemsCmd" Available="1" />
                  <CATCommandHeader ID="RestoreDeletedItemsHdr" ClassName="DS/LifecycleCmd/RestoreDeletedItemsCmd" ResourceFile="LifecycleCmd/RestoreDeletedItemsCmd" Available="1" />

            </CATCmdHeadersList>
      </Template>

      <Template syp:name="ENOFoldersWkbAccess" Target="CATCmdWorkbench">
            <CATCmdWorkbench Name="ENOFoldersWkb">
                  <CATCmdContainer Name="Folder" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="New Folder" Command="ActionBar_NewFolderCmd" />
                        <CATCmdStarter Name="Add Existing Content" Command="AddExistingContent" />
                        <CATCmdStarter Name="Remove Content" Command="RemoveContent" />
                        <CATCmdStarter Name="Add to Favorites" Command="AddToFavorites" />
                        <CATCmdStarter Name="Active Folder" Command="ActiveFolder" />
                        <CATCmdSeparator Name="Folder_Separator" />
                        <CATCmdStarter Name="Attributes" Command="ActionBar_Attributes" />
                        <CATCmdStarter Name="Delete Content" Command="DeleteHdr" />
                        <CATCmdSeparator Name="Folder_Separator" />
                     	<CATCmdStarter Name="Copy" Command="Copy" />
                        <CATCmdStarter Name="Cut" Command="Cut" />
                     	<CATCmdStarter Name="Paste" Command="Paste" />
                   </CATCmdContainer>
                   <CATCmdContainer Name="Content" RepresentationType="AfrActionBarSection">
                         <CATCmdStarter Name="Add File" Command="AddFile" />
                         <CATCmdStarter Name="Download" Command="DownloadFile" />
                         <CATCmdStarter Name="Check Out" Command="CheckOutFile" />
                         <CATCmdStarter Name="Check In" Command="CheckInFile" />
                    </CATCmdContainer>
          		    <CATCmdContainer Name="View" RepresentationType="AfrActionBarSection">
                  		<CATCmdStarter Name="Toggle Left Panel" Command="ToggleLeftPanel"/>
                        <CATCmdContainer Name="XSCView" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="TreeListView" Command="treelistview"/>
                              <CATCmdStarter Name="GraphView" Command="graphview"/>
                              <CATCmdStarter Name="TileView" Command="tileview"/>
                              <CATCmdStarter Name="ThumbnailView" Command="thumbnailview"/>
                        </CATCmdContainer>
                        <CATCmdContainer Name="PADExpand" RepresentationType="AfrActionBarDropDown">
                          <CATCmdStarter Name="Expand All" Command="ExpandAll" />
                          <CATCmdStarter Name="Expand N Levels" Command="ExpandN" />
                          <CATCmdStarter Name="Collapse All" Command="CollapseAll" />
                        </CATCmdContainer>
                        <CATCmdContainer Name="PADSelect" RepresentationType="AfrActionBarDropDown">
                          <CATCmdStarter Name="Select All" Command="SelectAll" />
                          <CATCmdStarter Name="Select Children" Command="SelectChildren" />
                          <CATCmdStarter Name="Select Inversion" Command="SelectInversion" />
                          <CATCmdStarter Name="Select Parent" Command="SelectParent" />
                          <CATCmdStarter Name="Select Siblings" Command="SelectSiblings" />
                          <CATCmdStarter Name="Select Leaves" Command="SelectLeaves" />
                        </CATCmdContainer>
                        <CATCmdStarter Name="Fit All In" Command="FitAllIn" />
                        <CATCmdStarter Name="Reframe On" Command="ReframeOn" />
                        <CATCmdStarter Name="Overview" Command="ToggleOverview" />
                        <CATCmdStarter Name="Swap Thumbnail" Command="SwapThumbState" />
                        <CATCmdContainer Name="LayoutOrientation" RepresentationType="AfrActionBarDropDown">
                          <CATCmdStarter Name="LayoutDown" Command="LayoutDown" />
                          <CATCmdStarter Name="LayoutRight" Command="LayoutRight" />
                          <CATCmdStarter Name="LayoutUp" Command="LayoutUp" />
                          <CATCmdStarter Name="LayoutLeft" Command="LayoutLeft" />
                        </CATCmdContainer>
                        <!-- <CATCmdSeparator Name="Folder_Separator"/>                  
						<CATCmdStarter Name="Toggle Side Panel" Command="ToggleRightPanel"/> --> 
                  </CATCmdContainer>
                  <CATCmdContainer Name="FolderTools" RepresentationType="AfrActionBarSection">
                    <CATCmdStarter Name="Where Used" Command="WhereUsed" />
                    <CATCmdContainer Name="PrintDropDown" RepresentationType="AfrActionBarDropDown">
                          <CATCmdStarter Name="Print" Command="Print"/>
                          <CATCmdStarter Name="Print Options" Command="PrintOptions"/>
                    </CATCmdContainer>
                    <CATCmdStarter Name="ColumnVisibility" Command="ColumnVisibility" />
                  </CATCmdContainer>
            </CATCmdWorkbench>
      </Template>
</Styles>
