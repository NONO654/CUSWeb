<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

      <Template syp:name="ENOXSceneHeaders" Target="CATCmdHeadersList">
            <CATCmdHeadersList>
                  <!-- VIEW SECTION -->
                  <CATCommandHeader Behavior="CATAfrHeaderCheck" ID="RootPanel" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/view/_xNavRootInfoPanel" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="Home" HelpResourceFile="PADServices/PADServicesHelpForCommands" ClassName="DS/PADServices/commands/HomeCmd" ResourceFile="PADServices/PADServices" Available="1" />
                  <CATCommandHeader ID="treeview" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="graphview" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="tileview" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="thumbnailview" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />

                  <CATCommandHeader ID="ExpandAll" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="CollapseAll" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ExpandN" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />

                  <CATCommandHeader ID="SelectChildren" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="SelectAll" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="SelectInversion" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="SelectParent" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="SelectSiblings" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="SelectLeaves" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="1" />
                  <CATCommandHeader ID="CleanAll" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader Behavior="CATAfrHeaderCheck" ID="RightSidePanel" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/view/xNavRightSidePanel" ResourceFile="PADUtils/PADUtils" Available="1">
                        <CATAfrCommandHeaderArgs>
                              <CATCommandHeaderArg ID="PropertiesReadOnly" Value="true" Mode="In" />
                        </CATAfrCommandHeaderArgs>
                  </CATCommandHeader>
                  <!-- GRAPH SECTION-->
                  <CATCommandHeader ID="ReframeAll" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ReframeOn" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ToggleOverview" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="LayoutDown" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="LayoutRight" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="LayoutUp" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="LayoutLeft" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="SwapThumbState" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />

                  <!-- TOOLS SECTION -->
                  <CATCommandHeader ID="RelationIPExplorer" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" />
                  <CATCommandHeader ID="DefineFilter" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/tools/xNavFilterCmds" ResourceFile="ENONextGenFilterCmd/ENONextGenFilterCmd" Available="1">
                        <CATAfrCommandHeaderArgs>
                              <CATAfrCommandHeaderArg ID="AuthorizeConfFilter" Value="true" Mode="In" />
                        </CATAfrCommandHeaderArgs>
                  </CATCommandHeader>
                  <CATCommandHeader ID="Print" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="PrintOptions" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ExportCSV" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ExportCSVOptions" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" Available="0" />
                  <CATCommandHeader ID="ColumnVisibility" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" />
                  <CATCommandHeader ID="Find" HelpResourceFile="ENXNav/ENXNavHelpForCommands" ClassName="DS/ENXNav/commands/xNavActionBarCmds" ResourceFile="ENXNav/ENXNav" />
                  <!-- Properties -->
                  <CATCommandHeader ID="ActionBar_Attributes" ClassName="DS/ENOCollabSharingCmds/commands/EditPropCmd/ActionBar_EditPropCmd" ResourceFile="ENOCollabSharingCmds/CollabSharing_Workbench" Available="1" />

                  <!-- OpenInCAD for web in win integration -->
                  <CATCommandHeader ID="OpenInCADHdr" ClassName="DS/PADUtils/commands/PADOpenInCAD" ResourceFile="PADUtils/PADUtils" Available="1" />
                  <!-- Just Command not in Action Bar -->
                  <CATCommandHeader ID="FilterDropObjects" ClassName="DS/ENXNav/commands/FilterDropObjects" ResourceFile="ENXNav/ENXNav" Available="1" />
            </CATCmdHeadersList>
      </Template>

      <Template syp:name="ENOXSceneWkbAccess" Target="CATCmdWorkbench">
            <CATCmdWorkbench Name="ENOXSceneWkb">
                  <CATCmdContainer Name="HomeFixedArea" RepresentationType="AfrActionBarFixedSection">
                        <CATCmdStarter Name="Home" Command="Home" />
                  </CATCmdContainer>
                  <CATCmdContainer Name="XSCViews" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="RootPanel" Command="RootPanel" />
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdContainer Name="XSCView" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="TreeView" Command="treeview" />
                              <CATCmdStarter Name="GraphView" Command="graphview" />
                              <CATCmdStarter Name="TileView" Command="tileview" />
                              <CATCmdStarter Name="ThumbnailView" Command="thumbnailview" />
                        </CATCmdContainer>
                        <CATCmdContainer Name="XSCExpand" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Expand All" Command="ExpandAll" />
                              <CATCmdStarter Name="Expand N" Command="ExpandN" />
                              <CATCmdStarter Name="Collapse All" Command="CollapseAll" />
                        </CATCmdContainer>

                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdContainer Name="XSCSelect" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Select All" Command="SelectAll" />
                              <CATCmdStarter Name="Select Children" Command="SelectChildren" />
                              <CATCmdStarter Name="Select Inversion" Command="SelectInversion" />
                              <CATCmdStarter Name="Select Parent" Command="SelectParent" />
                              <CATCmdStarter Name="Select Siblings" Command="SelectSiblings" />
                              <CATCmdStarter Name="Select Leaves" Command="SelectLeaves" />
                        </CATCmdContainer>
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Name="Fit All In" Command="ReframeAll" />
                        <CATCmdStarter Name="Reframe On" Command="ReframeOn" />
                        <CATCmdStarter Name="Overview" Command="ToggleOverview" />
                        <CATCmdStarter Name="SwapThumbState" Command="SwapThumbState" />
                        <CATCmdContainer Name="LayoutOrientation" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="LayoutDown" Command="LayoutDown" />
                              <CATCmdStarter Name="LayoutRight" Command="LayoutRight" />
                              <CATCmdStarter Name="LayoutUp" Command="LayoutUp" />
                              <CATCmdStarter Name="LayoutLeft" Command="LayoutLeft" />
                        </CATCmdContainer>
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Name="Clean All" Command="CleanAll" />
                        <CATCmdSeparator Name="PadViews_Separator" />
                        <CATCmdStarter Name="Toggle Side Panel" Command="RightSidePanel" />
                  </CATCmdContainer>
                  <CATCmdContainer Name="XSCTools" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="Attributes" Command="ActionBar_Attributes" />
                        <CATCmdStarter Name="RelationIPExplorer" Command="RelationIPExplorer" />
                        <CATCmdStarter Name="Define Filter" Command="DefineFilter" />
                        <CATCmdStarter Name="Find" Command="Find" />
                        <CATCmdContainer Name="PrintDropDown" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Print" Command="Print" />
                              <CATCmdStarter Name="Print Options" Command="PrintOptions" />
                        </CATCmdContainer>
                        <CATCmdContainer Name="ExportDropDown" RepresentationType="AfrActionBarDropDown">
                              <CATCmdStarter Name="Export CSV" Command="ExportCSV" />
                              <CATCmdStarter Name="Export CSV Options" Command="ExportCSVOptions" />
                        </CATCmdContainer>
                        <CATCmdStarter Name="ColumnVisibility" Command="ColumnVisibility" />
                  </CATCmdContainer>
            </CATCmdWorkbench>
      </Template>
</Styles>
