            <CATCommandHeader ID="SectionNormalToYHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1" Icon="DMUBaseCommands/assets/I_SectionSwitchXYZ.png"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionToY" Value = "Y" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
