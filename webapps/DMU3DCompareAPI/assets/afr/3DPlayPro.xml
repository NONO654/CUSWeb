            <CATCommandHeader ID="GraphicPropHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionColor" Value = "Color" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
