            <CATCommandHeader ID="VisuDarkGreyEnv" ClassName="DS/ViewerCommands/VisuDarkGreyEnvCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1" />
            <CATCommandHeader ID="VisuShinyEnv" ClassName="DS/ViewerCommands/VisuShinyEnvCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1" />
            <!---->
            <CATCommandHeader ID="GridHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionGrid" Value = "sectionGrid" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
            <CATCommandHeader ID="CutHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionCut" Value = "sectionCut" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
            <CATCommandHeader ID="NoCutHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionCutPlane" Value = "sectionCutPlane" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
            <CATCommandHeader ID="FlipHdr" ClassName="DS/DMUSection/DMUContextSectionCmd" SharedLibraryName="ShareUI" ResourceFile="DMUBaseCommands/3DPlayPro" Available="1"><CATAfrCommandHeaderArgs><CATCommandHeaderArg ID = "sectionFlip" Value = "sectionFlip" Mode = "In" /></CATAfrCommandHeaderArgs></CATCommandHeader>
