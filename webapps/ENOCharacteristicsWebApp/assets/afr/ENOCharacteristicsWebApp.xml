<?xml version="1.0" encoding="utf-8" ?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

      <Template syp:name="ENOCharacteristicsHeaders" Target="CATCmdHeadersList">
            <CATCmdHeadersList>
                  <CATCommandHeader ID="CreateCharacteristics" ClassName="DS/ENOCharacteristicsWebApp/commands/CreateCharacteristics" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="CreateFromMaster" ClassName="DS/ENOCharacteristicsWebApp/commands/CreateFromMaster" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="EvaluateCriteria" ClassName="DS/ENOCharacteristicsWebApp/commands/EvaluateCriteria" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="DeleteCharacteristcs" ClassName="DS/ENOCharacteristicsWebApp/commands/DeleteCharacteristcs" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="FilterCharacteristics" ClassName="DS/ENOCharacteristicsWebApp/commands/FilterCharacteristics" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="RefreshCharacteristics" ClassName="DS/ENOCharacteristicsWebApp/commands/RefreshCharacteristics" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
                  <CATCommandHeader ID="Export" ClassName="DS/ENOCharacteristicsWebApp/commands/Export" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" /> 
                  <CATCommandHeader ID="EditColumnSettings" ClassName="DS/ENOCharacteristicsWebApp/commands/EditColumnSettings" ResourceFile="ENOCharacteristicsWebApp/ENOCharacteristicsWebApp" Available="1" />
            </CATCmdHeadersList>
      </Template>

      <Template syp:name="ENOCharacteristicsWkbAccess" Target="CATCmdWorkbench">
            <CATCmdWorkbench Name="CharacteristicsWebApp">
                  <CATCmdContainer Name="Authoring" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="Create Characteristics" Command="CreateCharacteristics" />
                        <CATCmdStarter Name="Create From Master" Command="CreateFromMaster" />
                        <CATCmdStarter Name="Evaluate Criteria" Command="EvaluateCriteria" />
                        <CATCmdStarter Name="Delete Characteristics" Command="DeleteCharacteristcs" />
                        <CATCmdSeparator Name="Folder_Separator" />
                        <CATCmdStarter Name="Filter Characteristics" Command="FilterCharacteristics" />
                        <CATCmdStarter Name="Refresh Characteristics" Command="RefreshCharacteristics" />
                  </CATCmdContainer> 
                  <CATCmdContainer Name="Tools" RepresentationType="AfrActionBarSection">
                        <CATCmdStarter Name="Edit Table View" Command="EditColumnSettings" />
                        <CATCmdStarter Name="Tools" Command="Export" />
                  </CATCmdContainer>
            </CATCmdWorkbench>
      </Template>
</Styles>
