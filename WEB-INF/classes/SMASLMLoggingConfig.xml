<included><appender class="ch.qos.logback.core.rolling.RollingFileAppender" name="SMASLM_Appender"><file>${MX_TRACE_FILE_PATH}/${MX_TRACE_FILE_NAME:-3DXServer}_SMASLM_Active.log</file><rollingPolicy class="ch.qos.logback.core.rolling.SizeAndTimeBasedRollingPolicy"><fileNamePattern>${MX_TRACE_FILE_PATH}/${MX_TRACE_FILE_NAME:-3DXServer}_SMASLM_%d{yyyy-MM-dd_HH, UTC}_%i.log</fileNamePattern><maxFileSize>64MB</maxFileSize></rollingPolicy><encoder><pattern>%d{yyyy-MM-dd HH:mm:ss.SSS,GMT} | %-24([%thread]) | %-5level | %class{50} | %method | %line | %logger{20} | %msg%n%ex</pattern></encoder></appender><logger additivity="FALSE" level="OFF" name="SMASLM_Logger"><appender-ref ref="SMASLM_Appender"/></logger></included>